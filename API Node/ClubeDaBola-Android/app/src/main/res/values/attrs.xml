<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="CustomViewEditText">
        <attr name="android:inputType"/>
        <attr name="android:imeOptions"/>
        <attr name="android:maxLines"/>
        <attr name="android:maxLength"/>
        <attr name="edt_inputFocusedColor" format="color"/>
        <attr name="edt_inputUnFocusedColor" format="color"/>
        <attr name="edt_inputMarginHorizontal" format="integer"/>
        <attr name="edt_textColor" format="color"/>
        <attr name="edt_inputMarginBottom" format="integer"/>
        <attr name="edt_inputUnfocusedColorEdited" format="color"/>
        <attr name="edt_hintColorFocused" format="color"/>
        <attr name="edt_hintColorUnfocused" format="color"/>
        <attr name="edt_hintText" format="string"/>
        <attr name="edt_textSize" format="dimension"/>
        <attr name="textStyle" format="enum">
            <enum name="REGULAR" value="0"/>
            <enum name="LIGTH" value="1"/>
            <enum name="MEDIUM" value="2"/>
        </attr>
        <attr name="edt_hintLabelLeft" format="string"/>

    </declare-styleable>

    <declare-styleable name="ItemViewLabel">
        <attr name="item_hintText" format="string"/>
    </declare-styleable>


</resources>